generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Container {
  id        String   @id @default(uuid())
  dockerId  String   @unique
  name      String
  version   String
  port      Int
  username  String
  password  String
  status    String
  url       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
